{
  "bootloader": {
    "efuse": "0xFD",
    "file": "optiboot/optiboot_atmega328-Mini.hex",
    "hfuse": "0xD8",
    "lfuse": "0xFF",
    "lock_bits": "0x0F",
    "unlock_bits": "0x3F"
  },
  "build": {
    "core": "arduino",
    "extra_flags": "-DARDUINO_AVR_MINI",
    "f_cpu": "16000000L",
    "mcu": "atmega328p",
    "variant": "eightanaloginputs"
  },
  "debug": {
    "avr_debugger": true,
    "simavr_target": "atmega328p"
  },
  "frameworks": [
    "arduino"
  ],
  "name": "Arduino Mini ATmega328",
  "upload": {
    "maximum_ram_size": 2048,
    "maximum_size": 28672,
    "protocol": "arduino",
    "require_upload_port": true,
    "speed": 115200
  },
  "url": "http://arduino.cc/en/Main/ArduinoBoardMini",
  "vendor": "Arduino"
}